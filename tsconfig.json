{
	"compilerOptions": {
		"jsx": "react",
		"target": "ES2022",
		"module": "ES2022",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"plugins": [{ "name": "typescript-plugin-css-modules" }]
	}
}
